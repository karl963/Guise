<?xml version="1.0" encoding="UTF-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee                http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
   
   <servlet>
        <servlet-name>CandidateServlet</servlet-name>
        <servlet-class>guise.valimised.server.CandidateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CandidateServlet</servlet-name>
        <url-pattern>/CandidateServlet/*</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ValidateServlet</servlet-name>
        <servlet-class>guise.valimised.server.ValidateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ValidateServlet</servlet-name>
        <url-pattern>/ValidateServlet/*</url-pattern>
    </servlet-mapping>
    
       <servlet>
        <servlet-name>HääletusServlet</servlet-name>
        <servlet-class>guise.valimised.server.HääletusServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HääletusServlet</servlet-name>
        <url-pattern>/HääletusServlet/*</url-pattern>
    </servlet-mapping>
    
  <!-- Servlets -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>guise.valimised.server.GreetingServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/guise/greet</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Guise.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>SystemServiceServlet</servlet-name>
    <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
    <init-param>
      <param-name>services</param-name>
      <param-value/>
    </init-param>
  </servlet>

  <servlet-mapping>
    <servlet-name>SystemServiceServlet</servlet-name>
    <url-pattern>/_ah/spi/*</url-pattern>
  </servlet-mapping>

</web-app>